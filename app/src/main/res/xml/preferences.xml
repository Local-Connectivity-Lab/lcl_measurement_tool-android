<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto"

    >
    <PreferenceCategory app:title="General">
        <Preference
            app:key="export_signal"
            app:title="Export Signal Strength Data"
            app:summary="Export signal Strength Data in CSV format"
            app:icon="@drawable/export"
            app:enabled="false"/>
        <Preference
            app:key="export_measurement"
            app:title="Export Measurement Data"
            app:summary="Export signal Strength Data in CSV format"
            app:icon="@drawable/export"
            app:enabled="false"/>

    </PreferenceCategory>
    <PreferenceCategory
        app:title="Help">
        <Preference
            app:key="feedback"
            app:title="Send feedback"
            app:icon="@drawable/mail"
            app:summary="Report technical issues or suggest new features"/>
        <Preference
            app:title="Terms of Use"
            app:icon="@drawable/info"
            app:key="tou" />
        <Preference
            app:title="Privacy Policy"
            app:icon="@drawable/info"
            app:key="privacy"/>
        <Preference
            app:title="Logout"
            app:key="logout" />
        <Preference app:key="version" app:title="V1.0" app:summary="By Local Connectivity Lab - UW" app:enabled="false"/>

    </PreferenceCategory>

</PreferenceScreen>